@page
@{
    ViewData["Title"] = "2019 STEM Camp for Girls";
}

<section class="best-room">
    <div class="container">
        <div class="title-main">
            <h2 class="h2">
                @ViewData["Title"]
            </h2>
        </div>
        <p class="lead">
            Announcing our 2019 STEM Camp for girls hosted by <a href="https://www.linkedin.com/in/jennifer-stimpson-0668986/" target="_blank">Jennifer Stimpson</a> of <a href="http://jstempscience.com/" target="_blank">JSTEMP Science</a>!
        </p>
        <div class="embed-responsive embed-responsive-16by9">
            <iframe class="embed-responsive-item" src="https://www.youtube.com/embed/M9iwtNeaarw" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
        </div>
        <p>
            To learn more about Jennifer and JSTEMP Science, please check out this <a href="http://voyagedallas.com/interview/meet-jennifer-stimpson-jstemp-science-oak-cliff/" target="_blank">interview</a> featuring her.
        </p>
        <p>
            The application and more details about this camp will come soon. While you are waiting, you can <a href="http://eepurl.com/dBrIBT" target="_blank">sign up for our newsletter</a>.
        </p>
        <div class="col-xs-4">
            <img class="img-responsive" src="~/img/stem.jpeg" />
        </div>

        <div class="col-xs-4">
            <img class="img-responsive" src="~/img/stem2.jpeg" />
        </div>

        <div class="col-xs-4">
            <img class="img-responsive" src="~/img/jstemp.jpg" />
        </div>
    </div>
</section>